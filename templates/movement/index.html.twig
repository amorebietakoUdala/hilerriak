{% extends 'base.html.twig' %}

{% block title %}{{ 'app.title.movement.index' | trans }}{% endblock %}

{% block body %}
	<main class="container-fluid">
        {{ include('movement/_searchForm.html.twig')}}
        <div id="content" class="container-fluid" {{ stimulus_controller('table', 
                    { 'url': path('movement_index'),
                    'exportName': 'movements',
                })}} data-action="entity:success->table#refreshContent" data-table-target="content">
                {{ include('movement/_list.html.twig', { 
                    'movements': movements,
                    'pageSize' : pageSize,
                    'page': page,
                    'sortName': sortName,
                    'sortOrder': sortOrder,
                }) }}
        </div>
	</main>
{% endblock %}