{% if historyMovements is defined and historyMovements|length > 0 %}
<div>
<table class="table"
   data-toggle="table"
   data-table-target="table"
   data-page-number="{{ page }}" 
   data-page-size="{{ pageSize }}"
   data-page-sortName="{{ sortName }}"
   data-page-sortOrder="{{ sortOrder }}">
   <thead>
      <tr>
         {# <th data-sortable="true">{{ 'Id' | trans }}</th> #}
         <th data-sortable="true">{{ 'historyMovement.cementerio' | trans }}</th>         
         <th data-sortable="true">{{ 'historyMovement.numRegistro' | trans }}</th>
         <th data-sortable="true">{{ 'historyMovement.anyo' | trans }}</th>
         <th data-sortable="true">{{ 'historyMovement.tipoAccion' | trans }}</th>
         <th data-sortable="true">{{ 'historyMovement.sepulturaDestino' | trans }}</th>
         <th data-sortable="true">{{ 'historyMovement.fechaRegistro' | trans }}</th>
         <th data-sortable="true">{{ 'historyMovement.difunto' | trans }}</th>
         <th data-sortable="true">{{ 'historyMovement.descripcion' | trans }}</th>
         <th data-sortable="true">{{ 'historyMovement.origenRestos' | trans }}</th>
         <th data-sortable="true">{{ 'historyMovement.incidencias' | trans }}</th>
         <th data-sortable="true" data-field="options">{{"table.options" | trans }}</th>
      </tr>
   </thead>
   <tbody>
      {% for historyMovement in historyMovements %}
         <tr>
               {# <td>{{ historyMovement.id }}</td> #}
               <td>{{ historyMovement.cementerio }}</td>
               <td>{{ historyMovement.numRegistro }}</td>
               <td>{{ historyMovement.anyo }}</td>
               <td>{{ historyMovement.tipoAccion }}</td>
               <td>{{ historyMovement.sepulturaDestino }}</td>
               <td>{{ historyMovement.fechaRegistro | date('Y-m-d')}}</td>
               <td>{{ historyMovement.difunto }}</td>
               <td>{{ historyMovement.descripcion }}</td>
               <td>{{ historyMovement.origenRestos }}</td>
               <td>{{ historyMovement.incidencias }}</td>
               <td>
                  <a href="{{ path('historyMovement_show', {'historyMovement': historyMovement.id}) }}" data-action="table#click">
                     <i class="fa fa-eye" title='{{ "btn.show" | trans | capitalize }}'></i>
                  </a>
               </td>
         </tr>
      {% endfor %}
   </tbody>
</table>
{% else %}
   <div class="alert alert-secondary mt-3" role="alert">{{"messages.results0" | trans }}</div>
{% endif %}
{{ include('common/_alert.html.twig') }}
</div>

