{% extends 'base.html.twig' %}

{% block title %}{{ 'app.title.historyMovement.index' | trans }}{% endblock %}

{% block body %}
	<main class="container-fluid">
        {{ include('history_movements/_searchForm.html.twig')}}
        <div id="content" class="container-fluid" {{ stimulus_controller('table', 
                    { 'url': path('historyMovement_index'),
                    'exportName': 'historyMovements',
                })}} data-table-target="content">
                {{ include('history_movements/_list.html.twig', { 
                    'historyMovements': historyMovements,
                    'pageSize' : pageSize,
                    'page': page,
                    'sortName': sortName,
                    'sortOrder': sortOrder,
                }) }}
        </div>
	</main>
{% endblock %}