{% extends 'base.html.twig' %}

{% block title %}{{ 'app.title.owner.index' | trans }}{% endblock %}

{% block body %}
	<main class="container-fluid">
        {{ include('owner/_searchForm.html.twig') }}
        <div id="content" class="container-fluid" {{ stimulus_controller('table', 
                    { 'url': path('owner_index'),
                    'exportName': 'owners',
                    'filters':  ( filters | length ) > 0 ? filters : {'':''},
                })}} data-action="entity:success->table#refreshContent" data-table-target="content">
                {{ include('owner/_list.html.twig', { 
                    'owners': owners,
                    'pageSize' : pageSize,
                    'page': page,
                    'sortName': sortName,
                    'sortOrder': sortOrder,
                }) }}
        </div>
	</main>
{% endblock %}